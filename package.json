	},
	"devDependencies": {
		"babel-cli": "^6.8.0",
	    "babel-core": "^6.8.0",
	    "babel-eslint": "^7.0.0",
	    "babel-loader": "^6.2.4",
	    "babel-preset-env": "^1.6.1",
	    "babel-preset-react": "^6.0.15",
	    "babel-preset-stage-0": "^6.1.18",
	    "babel-register": "^6.26.0",
	    "d3": "4.10.0",
	    "eslint": "^3.8.1",
